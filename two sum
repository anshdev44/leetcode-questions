
#include <iostream>
using namespace std;

int main() {
    int arr[1000];
    int n;
    cin >> n;

    // Input array
    for (int z = 0; z < n; z++) {
        cin >> arr[z];
    }

    int target;
    cin >> target;

    // Checking sum
    bool found = false;  
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) { 
            if (arr[i] + arr[j] == target) {
                cout << "Indices: " << i << ", " << j << endl;
                found = true;
                break; 
            }
        }
        if (found) break; 
    }

    if (!found) {
        cout << "No pairs found." << endl; 
    }

    return 0;
}

